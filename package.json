{
  "name": "bouillon",
  "version": "1.1.1",
  "description": "Simple JSON storage API for working with persistent data in various types of Node applications.",
  "main": "index.js",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "bundle": "rollup -c",
    "bundle:watch": "rollup -c --watch",
    "test": "mocha"
  },
  "keywords": [
    "JSON",
    "persistent",
    "storage",
    "store",
    "keep",
    "file",
    "atomic"
  ],
  "typings": "lib/index.d.ts",
  "author": "Robert Corponoi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/robertcorponoi/bouillon"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.4",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-typescript": "^7.3.3",
    "@types/write-file-atomic": "^2.1.2",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "sinon": "^7.3.2",
    "typescript": "^3.5.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.4",
    "write-file-atomic": "^3.0.0"
  }
}
